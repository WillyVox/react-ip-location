{
  "name": "react-ip-location",
  "version": "1.0.30",
  "description": "Get your ip address and location with just a single line of code; get location information of a provided ip address",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "rollup": "rollup -c --bundleConfigAsCjs",
    "version": "npm version patch",
    "pl": "rollup -c --bundleConfigAsCjs && npm version && npm publish"
  },
  "keywords": [
    "country",
    "country code",
    "ip address",
    "ip-location",
    "location",
    "javascript",
    "reactjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WillyVox/react-ip-location.git"
  },
  "author": "WillyVox",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.6",
    "react": "^18.2.0",
    "typescript": "^5.0.4",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "rollup": "^3.21.6",
    "rollup-plugin-dts": "^5.3.0",
    "tslib": "^2.5.0"
  }
}
